  <div>
  <table class="table table-striped table-hover ">
    <thead>
      <tr>
        <th>name</th>
        <th>Created At</th>
        <th>Updated At</th>
        <th>Last ID</th>
        <th># Fields</th>
      </tr>
    </thead>
    <tbody>
      <% @tables.each do |key, table| %>
      <tr>
        <td><%= link_to(key, "/table/#{key}") %></td> 
        <td><%= Time.at(table.created_at) %></td>
        <td><%= Time.at(table.updated_at) %></td>
        <td><%= table.last_id %></td>
        <td><%= table.fields.count %></td>
      </tr>
      <% end %>
    </tbody>
  </table>

  </div>